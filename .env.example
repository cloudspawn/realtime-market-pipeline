# Kafka (Confluent Cloud)
KAFKA_BOOTSTRAP_SERVERS=your-server.confluent.cloud:9092
KAFKA_API_KEY=your-api-key
KAFKA_API_SECRET=your-api-secret

# Topics
KAFKA_TOPIC_TRADES=market.trades.raw
KAFKA_TOPIC_PRICES=market.prices.enriched
KAFKA_TOPIC_DLQ=market.dlq

# GCP
GCP_PROJECT_ID=your-project-id
GCP_CREDENTIALS_PATH=secrets/gcp-key.json

# BigQuery
BQ_DATASET=market_data

# Binance
BINANCE_WS_URL=wss://stream.binance.com:9443
BINANCE_SYMBOLS=btcusdt,ethusdt,solusdt,adausdt,dotusdt,avaxusdt,linkusdt,maticusdt,xrpusdt,bnbusdt,dogeusdt,shibusdt,ltcusdt,atomusdt,nearusdt,aptusdt,arbusdt,opusdt,injusdt,suiusdt

# CoinGecko
COINGECKO_API_URL=https://api.coingecko.com/api/v3
COINGECKO_POLL_INTERVAL_SECONDS=60
COINGECKO_COINS=bitcoin,ethereum,solana,cardano,polkadot,avalanche-2,chainlink,polygon-ecosystem-token,ripple,binancecoin,dogecoin,shiba-inu,litecoin,cosmos,near,aptos,arbitrum,optimism,injective-protocol,sui

# Consumer
CONSUMER_BATCH_SIZE=100
CONSUMER_FLUSH_TIMEOUT_SECONDS=10

# Monitoring
PROMETHEUS_PORT=8000

# Airflow
# Generate Fernet key: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW_FERNET_KEY=your_fernet_key_here
# Default credentials (change in production)
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=admin

# PostgreSQL (Airflow metadata)
POSTGRES_PASSWORD=your_postgres_password